@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .

<https://github.com/YimRakHee>
    a foaf:Person ;
    foaf:name "YimRakHee" ;
    foaf:homepage <https://github.com/YimRakHee> .

<https://github.com/YimRakHee/CrushFuzz>
    a lv2:Plugin ,
        lv2:FuzzPlugin ;
    doap:name "CrushFuzz" ;
    doap:license <http://opensource.org/licenses/isc> ;
    doap:maintainer <https://github.com/YimRakHee> ;
    lv2:project <https://github.com/YimRakHee> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:port [
        a lv2:InputPort , lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "in" ;
        lv2:name "In"
    ] , [
        a lv2:OutputPort , lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "input_gain" ;
        lv2:name "Input Gain" ;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 10.0 ;
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "bit_depth" ;
        lv2:name "Bit Depth" ;
        lv2:default 16.0 ;
        lv2:minimum 1.0 ;
        lv2:maximum 24.0 ;
        units:unit units:bit ;
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "downsample" ;
        lv2:name "Downsample" ;
        lv2:default 1.0 ;
        lv2:minimum 1.0 ;
        lv2:maximum 50.0 ;
        lv2:portProperty pprops:logarithmic ;
        units:unit [
            a units:Unit ;
            rdfs:label "factor" ;
            units:symbol "x" ;
            units:render "%f x"
        ]
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "dry_level" ;
        lv2:name "Dry Level" ;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 2.0 ;
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "wet_level" ;
        lv2:name "Wet Level" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 2.0 ;
    ] .
